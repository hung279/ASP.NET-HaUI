@model WebApplication1.Models.CourseStudent

@{
    ViewBag.Title = "ViewCourse";
}

<h2>ViewCourse</h2>

<div>
    <h4>Course: @Model.course.name</h4>
    <hr />
    <dl class="dl-horizontal">
        <table class="table">
            <tr>
                <th>
                    name
                </th>
                <th>
                    adree
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model.students)
            {
                <tr>
                    <td>
                        @item.name
                    </td>
                    <td>
                        @item.address
                    </td>
                    
                </tr>
            }

        </table>
    </dl>
</div>

